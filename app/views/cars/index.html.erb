<p id="notice"><%= notice %></p>

<h1>My Cars</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th>Max speed</th>
      <th>Boost</th>
      <th>Power</th>
      <th>Powerdensity</th>
      <th>Size</th>
      <th>Weight</th>
      <th>Photo url</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cars.each do |car| %>
      <tr>
        <td><%= car.name %></td>
        <td><%= car.price %></td>
        <td><%= car.max_speed %></td>
        <td><%= car.boost %></td>
        <td><%= car.power %></td>
        <td><%= car.powerdensity %></td>
        <td><%= car.size %></td>
        <td><%= car.weight %></td>
        <td><%= car.photo_url %></td>
        <td><%= car.content %></td>
        <td><%= link_to 'Show', car %></td>
        <td><%= link_to 'Edit', edit_car_path(car) %></td>
        <td><%= link_to 'Destroy', car, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car', new_car_path %>
